<h1>Listing schedules</h1>

<table>
  <thead>
    <tr>
      <th>Week</th>
      <th>Date</th>
      <th>Visitor</th>
      <th>Home</th>
      <th>Visitor score</th>
      <th>Home score</th>
      <th>Winning team</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @schedules.each do |schedule| %>
      <tr>
        <td><%= schedule.week %></td>
        <td><%= schedule.date %></td>
        <td><%= schedule.visitor_id %></td>
        <td><%= schedule.home_id %></td>
        <td><%= schedule.visitor_score %></td>
        <td><%= schedule.home_score %></td>
        <td><%= schedule.winning_team_id %></td>
        <td><%= link_to 'Show', schedule %></td>
        <td><%= link_to 'Edit', edit_schedule_path(schedule) %></td>
        <td><%= link_to 'Destroy', schedule, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Schedule', new_schedule_path %>
